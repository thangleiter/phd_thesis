@article{Barnes2016,
  title = {Filter Function Formalism beyond Pure Dephasing and Non-{{Markovian}} Noise in Singlet-Triplet Qubits},
  author = {Barnes, Edwin and Rudner, Mark S. and Martins, Frederico and Malinowski, Filip K. and Marcus, Charles M. and Kuemmeth, Ferdinand},
  date = {2016-03-16},
  journaltitle = {Phys. Rev. B},
