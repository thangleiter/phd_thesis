  author = {Descamps, Thomas and Liu, Feng and Kindel, Sebastian and Otten, Ren√© and Hangleiter, Tobias and Zhao, Chao and Lepsa, Mihail Ion and Ritzmann, Julian and Ludwig, Arne and Wieck, Andreas D. and Kardyna≈Ç, Beata E. and Bluhm, Hendrik},
