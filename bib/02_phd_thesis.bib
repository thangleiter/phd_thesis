@online{Bartling2024,
  title = {Universal High-Fidelity Quantum Gates for Spin-Qubits in Diamond},
  author = {Bartling, H. P. and Yun, J. and Schymik, K. N. and family=Riggelen, given=M., prefix=van, useprefix=false and Enthoven, L. A. and family=Ommen, given=H. B., prefix=van, useprefix=false and Babaie, M. and Sebastiano, F. and Markham, M. and Twitchen, D. J. and Taminiau, T. H.},
